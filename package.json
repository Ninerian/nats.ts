{
  "name": "ts-nats",
  "version": "1.0.0",
  "description": "Typescript Node.js client for NATS, a lightweight, high-performance cloud native messaging system",
  "keywords": [
    "nats",
    "messaging",
    "pubsub",
    "publish",
    "subscribe",
    "queue",
    "distributed",
    "queueing"
  ],
  "homepage": "https://nats.io",
  "repository": {
    "type": "git",
    "url": "git@github.com:nats-io/ts-nats.git"
  },
  "bugs": {
    "url": "https://github.com/nats-io/ts-nats/issues"
  },
  "license": "Apache-2.0",
  "private": false,
  "author": {
    "name": "The NATS Authors"
  },
  "contributors": [],
  "main": "./index.js",
  "scripts": {
    "build": "npm run clean:dist && tsc",
    "clean": "npm run clean:dist",
    "clean:dist": "rm -rf lib",
    "copy:dist": "cp lib/src/nats.{d.ts,js} lib/",
    "cover": "nyc report --reporter=html && open coverage/index.html",
    "pack": "npm pack",
    "debugtest": "tsc && node $NODE_DEBUG_OPTION node_modules/.bin/ava --verbose -T 6500000 -m",
    "test": "tsc && ava --verbose -T 15000"
  },
  "engines": {
    "node": ">= 4.5.0"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "nuid": "^1.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.108",
    "@types/node": "^9.6.13",
    "ava": "^1.0.0-beta.6",
    "chai": "^4.1.2",
    "dependency-check": "2.5.x",
    "if-node-version": "^1.1.1",
    "nyc": "^12.0.2",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  },
  "typings": "./lib/nats.d.ts",
  "ava": {
    "require": [],
    "files": [
      "./lib/test/**/*.js",
      "!./lib/test/helpers/**/*.js"
    ],
    "sources": [
      "**/*.js"
    ]
  }
}
