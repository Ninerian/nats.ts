{
  "name": "ts-nats",
  "version": "1.0.0",
  "description": "Typescript Node.js client for NATS, a lightweight, high-performance cloud native messaging system",
  "keywords": [
    "nats",
    "messaging",
    "pubsub",
    "publish",
    "subscribe",
    "queue",
    "distributed",
    "queueing"
  ],
  "homepage": "https://nats.io",
  "repository": {
    "type": "git",
    "url": "git@github.com:nats-io/ts-nats.git"
  },
  "bugs": {
    "url": "https://github.com/nats-io/ts-nats/issues"
  },
  "license": "Apache-2.0",
  "private": false,
  "author": {
    "name": "The NATS Authors"
  },
  "contributors": [],
  "main": "./index.js",
  "scripts": {
    "build": "npm run clean:dist && tsc && npm run copy:dist",
    "clean": "npm run clean:dist",
    "copy:dist": "cp lib/src/nats.{d.ts,js} lib/",
    "clean:dist": "rm -rf lib",
    "test": "mocha --exit --timeout 15000 -r ts-node/register test/*",
    "pack": "npm pack"
  },
  "engines": {
    "node": ">= 4.5.0"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "nuid": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.13",
    "chai": "^4.1.2",
    "dependency-check": "2.5.x",
    "if-node-version": "^1.1.1",
    "mocha": "^5.1.1",
    "should": ">= 9.0.0",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  },
  "typings": "./lib/nats.d.ts"
}
